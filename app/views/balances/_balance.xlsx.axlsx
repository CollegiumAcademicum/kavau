present(balance) do |presented|
  sheet.add_row [
    balance.id,
    balance.credit_agreement_id,
    presented.credit_agreement_number,
    balance.date,
    presented.creditor_name,
    balance.start_amount,
    balance.deposits,
    balance.disburses,
    balance.interests_sum,
    balance.end_amount
  ]
end
